<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HTTPClient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HTTPClient.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="HTTPClient">
    <TabBar>
        <Tab>
            <ShellContent
                Title="PostView"
                ContentTemplate="{DataTemplate local:PostView}"
                Route="PostView" />
        </Tab>
        <Tab>
            <ShellContent
                Title="TodosView"
                ContentTemplate="{DataTemplate local:TodosView}"
                Route="TodosView" />
        </Tab>
    </TabBar>
</Shell>
